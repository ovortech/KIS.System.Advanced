<body>
    @{
        var Nome = KIS.System.Advanced.MVC.Support.Security.SessionPersister.User != null ? KIS.System.Advanced.MVC.Support.Security.SessionPersister.User.Nome : "";
        var Email = KIS.System.Advanced.MVC.Support.Security.SessionPersister.User != null ? KIS.System.Advanced.MVC.Support.Security.SessionPersister.User.Email : "";
        var fimSessao = KIS.System.Advanced.MVC.Support.Security.SessionPersister.User != null ? KIS.System.Advanced.MVC.Support.Security.SessionPersister.User.FimDaSessao.ToString() : "";
    }
    <div class="container">
        <header class="bp-header cf">
            <div class="dummy-logo">
                <div class="dummy-icon"></div>
                <a href="~/Home/">
                    <img src="~/Content/vortech_logo.png" class="brand_logo" alt="Logo">
                </a>
            </div>
            <div class="bp-header__main">



                <ul class="nav ml-auto">
                    <li class="nav-item active icon_espace-rig mr-10">
                        <svg width="2em" height="5em" viewBox="0 0 16 16" class="bi bi-person-fill" fill="white">
                            <path fill-rule="evenodd" d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                        </svg>
                    </li>
                    <li class="nav-item" style="color: white; margin-right: 80px;">
                        <p style="margin-top: 2rem;">@Nome</p>
                    </li>
                    <li class="nav-item" style="color: white;float: right; margin-right: 25px;">
                        <p style="margin-top: 2rem;">A Sessão finaliza as: <b> @fimSessao</b></p>
                    </li>
                    <li class="nav-item icon_espace-lef" style=" ">
                        <a href="~/Login/Deslogar">
                            <svg width="2em" height="5em" viewBox="0 0 16 16" class="bi bi-power" fill="white">

                                <path fill-rule="evenodd" d="M5.578 4.437a5 5 0 1 0 4.922.044l.5-.866a6 6 0 1 1-5.908-.053l.486.875z" />
                                <path fill-rule="evenodd" d="M7.5 8V1h1v7h-1z" />
                            </svg>
                        </a>
                    </li>

                </ul>



                <nav class="bp-nav">
                    <div class="icon-color collapse navbar-collapse" id="navbarResponsive">
                    </div>
                </nav>
            </div>
        </header>
        <button class="action action--open" aria-label="Open Menu">
            <svg width="1.5em" height="1.5em" viewBox="0 0 16 16" class="bi bi-list" fill="white" xmlns="http://www.w3.org/2000/svg">
                <path fill-rule="evenodd" d="M2.5 11.5A.5.5 0 0 1 3 11h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4A.5.5 0 0 1 3 7h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4A.5.5 0 0 1 3 3h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z" />
            </svg></span>
        </button>
        <nav id="ml-menu" class="menu">
            <button class="action action--close" aria-label="Close Menu">
                <svg width="1.5em" height="1.5em" viewBox="0 0 16 16" class="bi bi-x" fill="white" xmlns="http://www.w3.org/2000/svg">
