 <!-- Bootstrap core CSS -->
<link href="~/Content/bootstrap.min.css" rel="stylesheet">
<link href="~/Content/home.css" rel="stylesheet">

<body>
    <div class="row rowc">
        <div class="col-md-4 mb-5">
            <div class="card h-100 text-center box-shadow ">
                <div class="card-body venda">
                    <svg width="5em" height="5em" viewBox="0 0 16 16" class="bi bi-cart4 icon-style" fill="currentColor">
                        <path fill-rule="evenodd" d="M0 2.5A.5.5 0 0 1 .5 2H2a.5.5 0 0 1 .485.379L2.89 4H14.5a.5.5 0 0 1 .485.621l-1.5 6A.5.5 0 0 1 13 11H4a.5.5 0 0 1-.485-.379L1.61 3H.5a.5.5 0 0 1-.5-.5zM3.14 5l.5 2H5V5H3.14zM6 5v2h2V5H6zm3 0v2h2V5H9zm3 0v2h1.36l.5-2H12zm1.11 3H12v2h.61l.5-2zM11 8H9v2h2V8zM8 8H6v2h2V8zM5 8H3.89l.5 2H5V8zm0 5a1 1 0 1 0 0 2 1 1 0 0 0 0-2zm-2 1a2 2 0 1 1 4 0 2 2 0 0 1-4 0zm9-1a1 1 0 1 0 0 2 1 1 0 0 0 0-2zm-2 1a2 2 0 1 1 4 0 2 2 0 0 1-4 0z" />
                    </svg>
                </div>
                <div class="card-footer text-center bkp-venda">
                    @Html.ActionLink("REALIZAR VENDA", "Index", "Vendas", null, new { @class = "color-white" })
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-5">
            <div class="card h-100 text-center box-shadow ">
                <div class="card-body produto">
                    <svg width="5em" height="5em" viewBox="0 0 16 16" class="bi bi-box-seam icon-style" fill="currentColor">
                        <path fill-rule="evenodd" d="M8.186 1.113a.5.5 0 0 0-.372 0L1.846 3.5l2.404.961L10.404 2l-2.218-.887zm3.564 1.426L5.596 5 8 5.961 14.154 3.5l-2.404-.961zm3.25 1.7l-6.5 2.6v7.922l6.5-2.6V4.24zM7.5 14.762V6.838L1 4.239v7.923l6.5 2.6zM7.443.184a1.5 1.5 0 0 1 1.114 0l7.129 2.852A.5.5 0 0 1 16 3.5v8.662a1 1 0 0 1-.629.928l-7.185 2.874a.5.5 0 0 1-.372 0L.63 13.09a1 1 0 0 1-.63-.928V3.5a.5.5 0 0 1 .314-.464L7.443.184z" />
                    </svg>
                </div>
                <div class="card-footer text-center bkp-produto">
                    @Html.ActionLink("PRODUTOS", "Index", "Produto", null, new { @class = "color-white" })
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-5 ">
            <div class="card h-100 text-center box-shadow ">
                <div class="card-body historico">
                    <svg width="5em" height="5em" viewBox="0 0 16 16" class="bi bi-clock-history icon-style" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M8.515 1.019A7 7 0 0 0 8 1V0a8 8 0 0 1 .589.022l-.074.997zm2.004.45a7.003 7.003 0 0 0-.985-.299l.219-.976c.383.086.76.2 1.126.342l-.36.933zm1.37.71a7.01 7.01 0 0 0-.439-.27l.493-.87a8.025 8.025 0 0 1 .979.654l-.615.789a6.996 6.996 0 0 0-.418-.302zm1.834 1.79a6.99 6.99 0 0 0-.653-.796l.724-.69c.27.285.52.59.747.91l-.818.576zm.744 1.352a7.08 7.08 0 0 0-.214-.468l.893-.45a7.976 7.976 0 0 1 .45 1.088l-.95.313a7.023 7.023 0 0 0-.179-.483zm.53 2.507a6.991 6.991 0 0 0-.1-1.025l.985-.17c.067.386.106.778.116 1.17l-1 .025zm-.131 1.538c.033-.17.06-.339.081-.51l.993.123a7.957 7.957 0 0 1-.23 1.155l-.964-.267c.046-.165.086-.332.12-.501zm-.952 2.379c.184-.29.346-.594.486-.908l.914.405c-.16.36-.345.706-.555 1.038l-.845-.535zm-.964 1.205c.122-.122.239-.248.35-.378l.758.653a8.073 8.073 0 0 1-.401.432l-.707-.707z" />
                        <path fill-rule="evenodd" d="M8 1a7 7 0 1 0 4.95 11.95l.707.707A8.001 8.001 0 1 1 8 0v1z" />
                        <path fill-rule="evenodd" d="M7.5 3a.5.5 0 0 1 .5.5v5.21l3.248 1.856a.5.5 0 0 1-.496.868l-3.5-2A.5.5 0 0 1 7 9V3.5a.5.5 0 0 1 .5-.5z" />
                    </svg>
                </div>
                <div class="card-footer text-center bkp-historico">
                    @Html.ActionLink("HISTÓRICO DE VENDAS", "Index", "Historico", null, new { @class = "color-white" })
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-5">
            <div class="card h-100 text-center box-shadow ">
                <div class="card-body financeiro">
                    <svg width="5em" height="5em" viewBox="0 0 16 16" class="bi bi-cash-stack icon-style" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path d="M14 3H1a1 1 0 0 1 1-1h12a1 1 0 0 1 1 1h-1z" />
                        <path fill-rule="evenodd" d="M15 5H1v8h14V5zM1 4a1 1 0 0 0-1 1v8a1 1 0 0 0 1 1h14a1 1 0 0 0 1-1V5a1 1 0 0 0-1-1H1z" />
                        <path d="M13 5a2 2 0 0 0 2 2V5h-2zM3 5a2 2 0 0 1-2 2V5h2zm10 8a2 2 0 0 1 2-2v2h-2zM3 13a2 2 0 0 0-2-2v2h2zm7-4a2 2 0 1 1-4 0 2 2 0 0 1 4 0z" />
                    </svg>
                </div>
                <div class="card-footer text-center bkp-financeiro">
                    @Html.ActionLink("FINANCEIRO", "Index", "Financeiro", null, new { @class = "color-white" })
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-5 ">
            <div class="card h-100 text-center box-shadow ">
                <div class="card-body usuarios">
                    <svg width="5em" height="5em" viewBox="0 0 16 16" class="bi bi-people-fill icon-style" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M7 14s-1 0-1-1 1-4 5-4 5 3 5 4-1 1-1 1H7zm4-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm-5.784 6A2.238 2.238 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.325 6.325 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1h4.216zM4.5 8a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z" />
                    </svg>
                </div>
                <div class="card-footer text-center bkp-usuarios">
                    @Html.ActionLink("USUÁRIOS", "Index", "Usuario", null, new { @class = "color-white" })
                </div>
            </div>
        </div>
    </div>
</body>